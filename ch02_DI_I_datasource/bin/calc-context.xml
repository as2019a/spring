<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- bean 3개 등록 및 의존관계 설정 -->
	<bean id ="myphone" class="com.assign.phone.MyPhone" p:calculator="smartCalculator"/>
	<bean id ="basicCalculator" class="com.assign.calculator.BasicCalculator" />
	<bean id ="smartCalculator" class="com.assign.calculator.SmartCalculator" />
		
</beans>
