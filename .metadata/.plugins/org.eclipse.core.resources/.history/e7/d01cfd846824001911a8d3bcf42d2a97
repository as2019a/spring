package com.inc.controller;

import javax.servlet.http.HttpSession;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;

@Controller
public class EtiquetteController {
	
	@RequestMapping(value="/eittes", method=RequestMethod.GET)
	public String eittes() {
		return "/eittes";
	}
	
	/*@RequestMapping(value="", method=RequestMethod.POST)*/
	
	@RequestMapping(value="executives/onadmin", method=RequestMethod.GET)
	public String onadmin(HttpSession session) {
		return "executives/onadmin";
	}
	
	@RequestMapping(value="executives/executboard", method=RequestMethod.GET)
	public String executboard(HttpSession session) {
		return "executives/executboard";
	}
	
	@RequestMapping(value="executives/maps", method=RequestMethod.GET)
	public String executmaps(HttpSession session) {
		return "executives/maps";
	}
	
	@RequestMapping(value="executives/etiquettes", method=RequestMethod.GET)
	public String executetiquettes(HttpSession session) {
		return "executives/etiquettes";
	}
	
}
